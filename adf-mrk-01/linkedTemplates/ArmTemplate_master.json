{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mrk-01"
		},
		"AzureDataLakeStorage_linkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_linkedService'"
		},
		"onpremsqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'onpremsqlserver'",
			"defaultValue": "Integrated Security=False;Data Source=DESKTOP-E1LHSQ4;Initial Catalog=AdventureWorksLT2017;User ID=read_user"
		},
		"AzureDataLakeStorage_linkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mkdatalakegen2storage.dfs.core.windows.net/"
		},
		"AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0728-034058-ps4hn3l"
		},
		"AzureKeyVault_LinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-mrk-01.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mrk-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_linkedService_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_linkedService_accountKey')]"
					},
					"onpremsqlserver_connectionString": {
						"value": "[parameters('onpremsqlserver_connectionString')]"
					},
					"AzureDataLakeStorage_linkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_linkedService_properties_typeProperties_url')]"
					},
					"AzureKeyVault_LinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_LinkedService_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}