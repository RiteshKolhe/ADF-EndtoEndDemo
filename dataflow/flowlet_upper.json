{
	"name": "flowlet_upper",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "data"
				}
			],
			"scriptLines": [
				"input(output(",
				"          EmployeeID as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Department as string,",
				"          JoiningDate as string,",
				"          Salary as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> data",
				"data derive(FirstName = upper(FirstName)) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          EmployeeID,",
				"          FirstName,",
				"          LastName,",
				"          Email,",
				"          Phone,",
				"          Department,",
				"          JoiningDate,",
				"          Salary",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}